function  toneAct = plotStuffReinfProbe(toneFrame,tempTC,beh)

toneAct = zeros(443,60,100);
for i = 1:100; toneAct(:,:,i) = ...
        tempTC(:,toneFrame(i)-9:toneFrame(i)+50); end
meanFlu = mean(reshape(toneAct,443,[]),2);
stdFlu = std(reshape(toneAct,443,[]),0,2);
toneZ = (toneAct-repmat(meanFlu,1,60,100)) ./ repmat(stdFlu,1,60,100);
toneDff = (toneAct-repmat(meanFlu,1,60,100)) ./ repmat(meanFlu,1,60,100);
figure; plot(mean(mean(toneDff,3),1))

toneDffProbe = (logical(beh(:,13)));

end

